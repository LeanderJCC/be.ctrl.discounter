<?xml version="1.0"?>
<extension key="be.ctrl.discounter" type="module">
    <file>discounter</file>
    <name>CiviCRM discounter extension</name>
    <description>Re-formats price_set label(s) for membership with discount.</description>
    <license>AGPL-3.0</license>
    <maintainer>
        <author>Hechtermans Wouter</author>
        <email>wouter@ctrl.be</email>
    </maintainer>
    <urls>
        <url desc="Main Extension Page">https://github.com/kewljuice/be.ctrl.discounter</url>
        <url desc="Documentation">https://github.com/kewljuice/be.ctrl.discounter</url>
        <url desc="Support">http://www.ctrl.be/</url>
        <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
    </urls>
    <releaseDate>2018-01-19</releaseDate>
    <version>1.2</version>
    <develStage>stable</develStage>
    <compatibility>
        <ver>4.7</ver>
    </compatibility>
    <comments>Once installed: price sets with discounts applied are reformatted prettier.</comments>
    <civix>
        <namespace>CRM/ctrl/discounter</namespace>
    </civix>
    <requires>
        <ext>org.civicrm.module.cividiscount</ext>
    </requires>
</extension>
